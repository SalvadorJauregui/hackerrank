    // Complete the maximumToys function below.
    static int maximumToys(int[] prices, int k) {
        int items = 0, sum = 0;
        Arrays.sort(prices);
        for (; sum < k; items++){
            sum+=prices[items];
        }
        return --items;
    }

  
